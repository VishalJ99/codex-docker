# Copy this file to .env and fill in only what you need.
# The .env file is baked into the Docker image at build time.

# Optional for Codex API-key auth inside container:
# printenv OPENAI_API_KEY | codex login --with-api-key
OPENAI_API_KEY=

# Optional for Twilio MCP: https://console.twilio.com/
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM_NUMBER=
TWILIO_TO_NUMBER=

# Optional for Zen MCP (if enabled in mcp-servers.txt)
OPENROUTER_API_KEY=

# Optional: custom conda installation path on host
# Example: CONDA_PREFIX=/path/to/miniconda3
CONDA_PREFIX=

# Optional: additional conda directories (space-separated)
# Paths with "env" are exported in CONDA_ENVS_DIRS
# Paths with "pkg" are exported in CONDA_PKGS_DIRS
CONDA_EXTRA_DIRS=

# Optional: default Docker runtime settings for codex-docker wrapper
DOCKER_MEMORY_LIMIT=
DOCKER_GPU_ACCESS=
DOCKER_NETWORK_MODE=host

# Optional: additional apt packages to install in container image
# Example: SYSTEM_PACKAGES="libopenslide0 libgdal-dev"
SYSTEM_PACKAGES=
